{
  "name": "@sv-sdk/cache",
  "version": "0.0.1",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Big0290/sv-sdk.git",
    "directory": "packages/cache"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "description": "Redis caching and BullMQ queue system for SV-SDK",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.ts",
    "./cache-utils": "./src/cache-utils.ts",
    "./queue": "./src/queue.ts",
    "./config": "./src/config.ts",
    "./health": "./src/health.ts"
  },
  "scripts": {
    "dev": "tsc --watch --noEmit",
    "build": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@sv-sdk/shared": "workspace:*",
    "bullmq": "^5.28.1",
    "ioredis": "^5.4.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
