{
  "name": "@sv-sdk/security",
  "version": "0.0.1",
  "type": "module",
  "description": "Security utilities for SV-SDK (rate limiting, CSRF, sanitization)",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./rate-limiter": "./src/rate-limiter.ts",
    "./csrf": "./src/csrf.ts",
    "./sanitization": "./src/sanitization.ts",
    "./headers": "./src/headers.ts",
    "./secrets": "./src/secrets.ts"
  },
  "scripts": {
    "dev": "tsc --watch --noEmit",
    "build": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@sv-sdk/cache": "workspace:*",
    "@sv-sdk/shared": "workspace:*",
    "nanoid": "^5.0.9"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}

